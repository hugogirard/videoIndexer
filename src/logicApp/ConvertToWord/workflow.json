            "Select_needed_elements": {
                "type": "Select",
                "inputs": {
                    "from": "@body('Parse_JSON_first_video')?['insights']?['transcript']",
                    "select": {
                        "SpeakerId": "@item()?['speakerId']",
                        "Text": "@item()?['text']"
                    }
                },
                "runAfter": {
                    "Parse_JSON_first_video": [
                        "Succeeded"
                    ]
                }
