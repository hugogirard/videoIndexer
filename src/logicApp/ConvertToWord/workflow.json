            "Populate_Microsoft_Word_template_transcription": {
                "type": "ApiConnection",
                "inputs": {
                    "host": {
                        "connection": {
                            "referenceName": "wordonlinebusiness"
                        }
                    },
                    "method": "post",
                    "body": {
                        "940878764": "@{body('Select_needed_elements')}@{body('Select_needed_elements')}@{body('Select_needed_elements')}@{body('Select_needed_elements')}@{body('Select_needed_elements')}@{body('Select_needed_elements')}@{body('Select_needed_elements')}"
                    },
                    "path": "/api/templates/getFile",
                    "queries": {
                        "source": "me",
                        "drive": "b!5uOSMwdCWEOG55ZKclXYhKKad8ISIzVJjQBG2PoLwIuO7FpL9WCWQ6gnjzhINA4m",
                        "file": "01MWOW7Y3GPQTWODJZTRCYON5ZL23HTWEF"
                    }
                },
                "runAfter": {
                    "Select_needed_elements": [
                        "Succeeded"
                    ]
                },
                "metadata": {
                    "01MWOW7Y3GPQTWODJZTRCYON5ZL23HTWEF": "/Template/TemplateTranscription.docx"
                }
            },
            "Uploads_Word_Document_to_Azure_Storage": {
                "type": "ServiceProvider",
                "inputs": {
                    "parameters": {
                        "containerName": "transcript",
                        "blobName": "@{body('Parse_JSON_queue_message')?['name']}.docx",
                        "content": "@body('Populate_Microsoft_Word_template_transcription')"
                    },
                    "serviceProviderConfiguration": {
                        "connectionName": "AzureBlob",
                        "operationId": "uploadBlob",
                        "serviceProviderId": "/serviceProviders/AzureBlob"
                    }
                },
                "runAfter": {
                    "Populate_Microsoft_Word_template_transcription": [
                        "Succeeded"
                    ]
                }
            },
            "Parse_JSON_first_video": {
                "type": "ParseJson",
                "inputs": {
                    "content": "@first(body('Parse_JSON_queue_message')?['videos'])",
                    "schema": {
                        "type": "object",
                        "properties": {
